{
  "name": "hitechnix/php-cs-fixer",
  "description": "Automatically configuration PHP CS Fixer for projects.",
  "keywords": [
    "php",
    "hitechnix",
    "php-cs-fixer"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Hi-Technix, Inc.",
      "email": "support@hitechnix.com",
      "homepage": "https://opensource.hitechnix.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Hitechnix\\PhpCsFixer\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
